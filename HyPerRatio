name = getTitle();
run("Split Channels");
C1 = "C1-"+name;
C2 = "C2-"+name;
C3 = "C3-"+name;
close(C3);
selectWindow(C2);
run("Gaussian Blur...", "sigma=1");
run("32-bit");
selectWindow(C1);
run("Gaussian Blur...", "sigma=1");
run("32-bit");
setAutoThreshold("Default dark");
//run("Threshold...");
run("NaN Background");
imageCalculator("Divide create 32-bit", C2, C1);
selectWindow("Result of "+C2);
run("Measure");
